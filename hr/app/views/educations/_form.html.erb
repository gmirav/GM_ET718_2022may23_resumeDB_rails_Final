<%= form_for([@resume, @resume.educations.build]) do |f| %>
  <p>
    <%= f.label :institution %><br>
    <%= f.text_field :institution %>
  </p>
  <p>
    <%= f.label :start_to_finish %><br>
    <%= f.text_area :start_to_finish %>
  </p>
  <p>
    <%= f.label :achievement %><br>
    <%= f.text_area :achievement %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>